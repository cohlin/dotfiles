	email = christian.ohlinjansson@schibsted.com
[push]
	default = tracking
[color]
	ui = auto
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = blue bold
	new = green bold
[color "status"]
[alias]
	co = checkout
	br = branch
	ci = commit
	st = status
	last = log -1 HEAD
	plog = log --pretty=\"%C(Yellow)%h  %C(reset)%ad (%C(Green)%cr%C(reset))%x09 %C(Cyan)%an: %C(reset)%s\"
	pllog = log --pretty=format:\"%C(yellow)%h %ar %C(auto)%d %Creset %s , %Cblue%cn\" --graph --all
[core]
	editor = vim
